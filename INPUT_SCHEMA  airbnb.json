{
    "title": "Airbnb Scrapper input",
    "description": "To unleashed the full power of the scraper/API use the options bellow!",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
      "locationQuery": {
        "title": "Location",
        "type": "string",
        "description": "Location name as you would enter to Airbnb search. Actor takes the first result as desired location.",
        "default": "Sacramento",
        "prefill": "Sacramento",
        "editor": "textfield"
      },
      "startUrls": {
        "title": "Start URLs",
        "type": "array",
        "description": "Urls of Airbnb details (not search urls), when set the actor will ignore locationQuery and will return results only for these listings",
        "editor": "requestListSources"
      },
      "simple": {
        "title": "Extract main listing information only (no detail)",
        "type": "boolean",
        "description": "The crawler will get only the main listing information and skip other details.",
        "default": true
      },
      "includeReviews": {
        "title": "Include reviews",
        "type": "boolean",
        "description": "If checked results includes reviews data",
        "default": true
      },
      "maxReviews": {
        "title": "Maximum reviews",
        "type": "integer",
        "description": "Amount of reviews to be scraped for each listing.",
        "default": 10,
        "example": 10
      },
      "checkIn": {
        "title": "Checkin date",
        "type": "string",
        "description": "First day of stay. Format: YYYY-MM-DD",
        "example": "YYYY-MM-DD",
        "editor": "textfield"
      },
      "checkOut": {
        "title": "Checkout date",
        "type": "string",
        "description": "Last day of stay. Format: YYYY-MM-DD",
        "example": "YYYY-MM-DD",
        "editor": "textfield"
      },
      "minPrice": {
        "title": "Minimum price",
        "type": "integer",
        "description": "Sets filter min_price (per night) to the search query.",
        "default": 0,
        "example": 100
      },
      "maxPrice": {
        "title": "Maximum price",
        "type": "integer",
        "description": "Sets filter max_price (per night) to the search query.",
        "default": 100000,
        "example": 100000
      },
      "includeCalendar": {
        "title": "Include calendar",
        "type": "boolean",
        "description": "If checked, includes the available range of days",
        "default": false
      },
      "addMoreHostInfo": {
        "title": "Add more host information",
        "type": "boolean",
        "description": "If checked, add more information for the primary host such as host url and number of listings. Only work if the Extract main listing information only (no detail) is unchecked",
        "default": false
      },
