      "proxyConfiguration": {
        "title": "Proxy configuration",
        "type": "object",
        "description": "Use Apify Proxy or your custom Proxy",
        "prefill": {
          "useApifyProxy": true
        },
        "default": {
          "useApifyProxy": true
        },
        "editor": "proxy"
      },
      "maxListings": {
        "title": "Max listings",
        "type": "integer",
        "description": "Limit the amount of results you want. Omit this setting for no limit",
        "prefill": 10,
        "example": 10
      },
      "maxConcurrency": {
        "title": "Max concurrency",
        "type": "integer",
        "description": "Defines how many pages can be processed by the scraper in parallel. The scraper automatically increases and decreases concurrency based on available system resources. Use this option to set a hard limit.",
        "minimum": 1,
        "default": 50,
        "prefill": 50
      },
      "limitPoints": {
        "title": "Limit points",
        "type": "integer",
        "description": "Limits the number of geo points returned by the reverse API. The higher the number, the longer it takes. There's no upper limit",
        "minimum": 1,
        "default": 100,
        "prefill": 100
      },
      "timeoutMs": {
        "title": "Timeout milliseconds",
        "type": "integer",
        "description": "Override the default 60s (60.000ms) timeout when not using startUrls",
        "minimum": 60000,
        "default": 60000,
        "prefill": 60000
      },
      "debugLog": {
        "title": "Debug Log",
        "type": "boolean",
        "description": "Shows additional debugging information",
        "default": false
      }
